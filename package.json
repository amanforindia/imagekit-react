{
    "name": "imagekitio-react",
    "version": "1.1.5",
    "description": "React SDK for ImageKit.io which implements client-side upload and URL generation for use inside a react application.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "jest": {
        "verbose": true,
        "setupFilesAfterEnv": [
            "<rootDir>/tests/jest/setupTests.js"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/tests/jest/mocks"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/tests/cypress/"
        ]
    },
    "scripts": {
        "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "cypress:open": "cypress open",
        "test": "jest",
        "test:ci": "jest --coverage && codecov",
        "test:cov": "jest --coverage",
        "test:e2e": "cypress open",
        "test:watch": "jest --watch",
        "serve:test-app": "export SKIP_PREFLIGHT_CHECK=true;npx serve -s tests/test-app/build -p 4000"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/imagekit-developer/imagekit-react"
    },
    "author": "ImageKit",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.7.4",
        "@babel/core": "^7.7.4",
        "@babel/plugin-proposal-class-properties": "^7.7.4",
        "@babel/plugin-proposal-export-default-from": "^7.7.4",
        "@babel/preset-env": "^7.7.4",
        "@babel/preset-react": "^7.7.4",
        "@babel/register": "^7.7.4",
        "@babel/runtime": "^7.7.6",
        "@types/node": "^17.0.21",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "cypress": "^10.4.0",
        "cypress-file-upload": "^5.0.8",
        "jest": "^26.5.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "typescript": "^4.5.2",
        "codecov": "^3.8.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "babel-loader": "^8.0.6",
        "react-test-renderer": "^16.13.1",
        "require-context.macro": "^1.2.2",
        "sinon": "^9.2.0"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "dependencies": {
        "imagekit-javascript": "^1.5.2"
    }
}
